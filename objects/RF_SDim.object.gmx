                        Pers_Obj.sprite_index = sprTailsGun
                    if (global.Sprite_Mode == 2)
                        Pers_Obj.sprite_index = sprTailsBloody1_Gun
                    if (global.Sprite_Mode == 3)
                        Pers_Obj.sprite_index = sprTailsBloody2_Gun
                    if (Gun_ChargeTime &gt; 0)
                    {
                        if (Act == 0)
                        {
                            if (Pers_Obj.image_xscale == 1)
                                instance_create((Pers_Obj.bbox_right + 12), Pers_Obj.y, TTS1_Ball)
                            if (Pers_Obj.image_xscale == -1)
                                instance_create((Pers_Obj.bbox_left - 12), Pers_Obj.y, TTS1_Ball)
                            Act = 1
                            audio_play_sound(SFX_GCU, 1, false)
                        }
                        Gun_ChargeTime -= 1
                    }
                    else
                    {
                        Gun_Charge = 1
                        Act = 2
                    }
                }
                if keyboard_check_released(ord("C"))
                {
                    GOD_OBJECT.LD_Phase = 0
                    with (Pers_Obj)
                    {
                        FlyAble = 1
                        FlyTime = 220
                        Fly = 0
                        SpinRoll = 1
                        Up_Down = 1
                        Jump = 1
                        Move = 1
                        Grav = 1
                        Handle = 1
                    }
                    if (Act == 2)
                    {
                        if (Pers_Obj.image_xscale == 1)
                            global.vel = -6
                        if (Pers_Obj.image_xscale == -1)
                            global.vel = 6
                        if (Gun_Charge == 1)
                        {
                            TTS1_Ball.Act = 1
                            TTS1_Ball.Mode = 1
                            Act = 0
                            audio_stop_sound(SFX_GCU)
                            audio_play_sound(SFX_GS, 1, false)
                            Gun_ChargeTime = 140
                            Gun_Charge = 0
                        }
                    }
                    if (Act == 1)
                    {
                        if (Gun_Charge == 0)
                        {
                            TTS1_Ball.Act = 3
                            Act = 0
                            audio_stop_sound(SFX_GCU)
                            Gun_ChargeTime = 140
                            Gun_Charge = 0
                        }
                    }
                }
            }
        }
    }
